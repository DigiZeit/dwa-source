{
    "name" : "CarouselSample",
    "mode": "debug",
    "debugLevel": 1,
    "eliminate": false,
    "minify": false,
    "useTransition": false,
    "htmlHeader": [
        "<meta charset=\"utf-8\">",
        "<meta name=\"apple-mobile-web-app-capable\" content=\"yes\">",
        "<meta name=\"apple-mobile-web-app-status-bar-style\" content=\"default\">",
        "<link rel=\"apple-touch-icon-precomposed\" href=\"theme/images/Icon-72.png\" />",
        "<link rel=\"apple-touch-icon-precomposed\" href=\"theme/images/Icon.png\" />",
        "<link rel=\"apple-touch-icon-precomposed\" href=\"theme/images/Icon@2x.png\" />"

    ],
    "application": {
        "version": "0.0.1",
        "debugMode": true,
        "useTransitions": true,
        "enableTouchOverflow": true,
        "keyPrefix": "#m#",
        "keySuffix": "_",
        "timeStampCreated": "created_at",
        "timeStampUpdated": "updated_at",
        "m_id": "m_id"
    },

    "supportedLanguages": [
        "en_us",
        "de_de"
    ],

    "defaultLanguage": "de_de",

    "package": {
        "android": {
            "method": "PhoneGap",
            "os": "Android",
            "sdk": "android-8",
            "mode": "debug",
            "package": "com.tmp.activity",
            "activity": "Carousel",
            "keystore": "path/to/your/keystore",
            "keyalias": "testkey",
            "storepass": "test123",
            "manifest": {
                "activity": {
                    "screenOrientation": "portrait"
                }
            },
            "autoScaleIcon": true
        },
        "ios": {
            "method": "PhoneGap",
            "os": "iOS",
            "sdk": "iphonesimulator4.3",
            "package": "com.tmp.activity",
            "activity": "Carousel",
            "mode": "debug",
            "plists": {
                "PhoneGap": {
                    "DetectPhoneNumber": true,
                    "TopActivityIndicator": "gray",
                    "EnableLocation": false,
                    "EnableAcceleration": true,
                    "EnableViewportScale": false,
                    "AutoHideSplashScreen": true,
                    "ShowSplashScreenSpinner": true,
                    "MediaPlaybackRequiresUserAction": false,
                    "AllowInlineMediaPlayback": false,
                    "OpenAllWhitelistURLsInWebView": false,
                    "ExternalHosts": ["*"],
                    "Plugins": {
                        "com.phonegap.accelerometer": "PGAccelerometer",
                        "com.phonegap.camera": "PGCamera",
                        "com.phonegap.connection": "PGConnection",
                        "com.phonegap.contacts": "PGContacts",
                        "com.phonegap.debugconsole": "PGDebugConsole",
                        "com.phonegap.file": "PGFile",
                        "com.phonegap.filetransfer": "PGFileTransfer",
                        "com.phonegap.geolocation": "PGLocation",
                        "com.phonegap.notification": "PGNotification",
                        "com.phonegap.media": "PGSound",
                        "com.phonegap.mediacapture": "PGCapture",
                        "com.phonegap.splashscreen": "PGSplashScreen"
                    }
                }
            }
        }
    }
}
