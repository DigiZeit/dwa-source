            "method": "ftp",
            "ftp": {
                "deleteTargetPath": true,
                "host": "digi-gps.de",
                "port": 21,
                "username": "digiwebapp",
                "password": "Digizeit08",
                "targetPath": "/digiwebapp-testing/deployed/",
